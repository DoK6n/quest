{
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "desktop": "turbo run --filter desktop",
    "desktop:add": "pnpm add --filter desktop",
    "web:dev": "turbo run --filter web dev"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "tsconfig": "workspace:*",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.7.5",
  "name": "quest",
  "engines": {
    "node": ">18.16.0",
    "pnpm": ">=8.7.5"
  }
}
